# c-#include <iostream>
using namespace std;
int main() {
    //Update the value
    int x=10;
    int *ptr;
    int marks=100;
    ptr=&marks;
    cout<<*ptr<<endl;
    //Addresing store 
    int x=10;
    int y=10;
    int *ptr=&x;
    cout<<&x<<endl;
    cout<<ptr<<endl;
    cout<<&y;
    return 0;
}



//Swap with pointer

program online
#include <iostream>
using namespace std;
void swap(int *x,int *y){
    int temp=0;
    temp=*x;
    *x=*y;
    *y=temp;
}

int main() {
    int x=10;
    int y=20;
    int *p1=&x;
    int *p2=&y;
    swap(*p1,*p2);
    cout<<*p1<<" "<<*p2;
    return 0;
}
