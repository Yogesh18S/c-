// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int i,row,col;
    cin>>row;
    cin>>col;
    for(i=1;i<=row;i++){
        for(int j=1;j<=col;j++){
            if(i==1||j==1||i==row||j==col){
                cout<<i;
            }
            else{
                cout<<" ";
            }
          
        }
        cout<<endl;
       
    }
    return 0;
}
